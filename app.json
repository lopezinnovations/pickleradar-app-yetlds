{
  "expo": {
    "name": "PickleRadar",
    "slug": "PickleRadar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/1df577f2-3672-4bc4-a347-721aa5fb58e0.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/1df577f2-3672-4bc4-a347-721aa5fb58e0.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B4D3E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.PickleRadar",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "PickleRadar uses your location to show nearby pickleball courts.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/1df577f2-3672-4bc4-a347-721aa5fb58e0.png",
        "backgroundColor": "#1B4D3E"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.PickleRadar",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/1df577f2-3672-4bc4-a347-721aa5fb58e0.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "PickleRadar uses your location to show nearby pickleball courts."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/1df577f2-3672-4bc4-a347-721aa5fb58e0.png",
          "color": "#1B4D3E",
          "sounds": [],
